 Dowload file\r\nexport const download_drawing = function(el) {\r\n  const image = canvas.toDataURL(\"image/jpg\")\r\n  el.href = image\r\n}\r\nd.querySelector(\"#save_drawing\").addEventListener(\"click\", e => download_drawing(e.target))"},"lineCount":null}},"error":null,"hash":"01edcc884d88401f668c768c767c28ea","cacheData":{"env":{}}}